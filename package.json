{
  "name": "sRemote",
  "version": "0.1.0",
  "description": "SSH Terminal",
  "main": "main.js",
  "author": "100LAR STUDIO",
  "license": "ISC",
  "devDependencies": {
    "electron": "^26.6.2",
    "electron-builder": "^24.9.1",
    "electron-reloader": "^1.2.3",
    "ssh2": "^1.15.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.6",
    "electron-is-packaged": "^1.0.2",
    "electron-root-path": "^1.1.0",
    "ini": "^4.1.1",
    "ssh2": "^1.15.0"
  },
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "asar": true,
    "files": [
      "static/",
      "index.html",
      "ssh.html",
      "logo.svg",
      "logo.png",
      "logo.ico",
      "settings.js",
      "main.js"
    ],
    "appId": "sRemote",
    "win": {
      "target": "portable",
      "icon": "logo.ico"
    },
    "linux": {
      "target": "portable",
      "icon": "logo.ico"
    },
    "portable": {
      "unicode": false,
      "artifactName": "sRemote.exe"
    }
  }
}
